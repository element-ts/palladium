{
  "name": "@element-ts/palladium",
  "version": "2.0.5",
  "description": "A type-safe promise based http/s request library.",
  "keywords": [
    "request",
    "promise",
    "http",
    "https",
    "typescript",
    "type",
    "runtime",
    "network",
    "cloud",
    "api",
    "json"
  ],
  "main": "js/index",
  "types": "dts/index",
  "files": [
    "js",
    "dts"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && /usr/local/bin/tsc -p tsconfig.json",
    "watch": "tsc -w -p .",
    "clean": "rm -rf js && rm -rf dts",
    "start": "node js/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element-ts/palladium.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Elijah Cobb <elijah@elijahcobb.com> (https://elijahcobb.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/element-ts/palladium/issues"
  },
  "homepage": "https://github.com/element-ts/palladium#readme",
  "dependencies": {
    "@element-ts/oxygen": "latest",
    "@element-ts/neon": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "typescript": "latest",
    "tslint": "latest",
    "jest": "latest",
    "@types/jest": "latest"
  }
}
